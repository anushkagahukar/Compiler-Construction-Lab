%{
#include<stdio.h>
int counter = 0;
int counter1=0;
%}
%%
^.*com$ {counter++;}
.*|\n {counter1++;}
%%
int main(){
printf("Enter your input: \n");
yylex();
printf("Number of line ending with 'com' = %d\n", counter);
printf("Number of line not ending with 'com' = %d\n", counter1);
return 0;
}
int yywrap()
{
return 1;
}
